<h2>Edit Historical Landmark</h2>

  <form action="/landmarks/<%=@landmark.id%>" method="post">
    <input type="hidden" id="hidden" name="_method" value="patch">
    <label>Name: </label><input type="text" name="landmark[name]" id="name">
    <label>Year Completed: </label><input type="integer" name="landmark[year_completed]" id="year_completed" placeholder="<%=@landmark.year_completed.to_s%>">


    <input type="submit" value="Edit Landmark">

  </form>

<a class="button" href="/landmarks">Landmarks</a>
<a class="button" href="/">Home</a>
